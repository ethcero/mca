[Unit]
 Description = Practica1
 After network.target = practica1.service
[Service]
 Type = forking
 Restart=always
 RestartSec=1
 SuccessExitStatus=143
 ExecStart = /usr/local/bin/practica1.sh start
 ExecStop = /usr/local/bin/practica1.sh stop
 ExecReload = /usr/local/bin/practica1.sh reload
[Install]
 WantedBy=multi-user.target